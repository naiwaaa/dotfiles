{
  "[Log]": {
    "editor.fontSize": 11
  },
  "[lua]": {
    "editor.defaultFormatter": "JohnnyMorganz.stylua"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "other": "on",
      "strings": "on"
    }
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[tmpl]": {
    "editor.formatOnSave": false
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "code-runner.clearPreviousOutput": true,
  "code-runner.enableAppInsights": false,
  "code-runner.executorMapByGlob": {
    "pom.xml": "cd $dir && mvn clean package",
    "test_*.zig": "zig test"
  },
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "csv-preview.resizeColumns": "all",
  "debug.console.fontSize": 11,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.wordWrap": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.cursorWidth": 3,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.find.addExtraSpaceOnTop": false,
  "editor.fontFamily": "'JetBrainsMono NF', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.lineHeight": 20,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.padding.top": 16,
  "editor.quickSuggestions": {
    "comments": "off",
    "other": "on",
    "strings": "on"
  },
  "editor.renderFinalNewline": "dimmed",
  "editor.renderLineHighlight": "all",
  "editor.renderLineHighlightOnlyWhenFocus": true,
  "editor.scrollbar.horizontalScrollbarSize": 8,
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.semanticHighlighting.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.unfoldOnClickAfterEndOfLine": true,
  "editor.unicodeHighlight.includeComments": true,
  "editor.unicodeHighlight.nonBasicASCII": true,
  "eslint.experimental.useFlatConfig": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "eslint.run": "onSave",
  "evenBetterToml.formatter.alignComments": true,
  "evenBetterToml.formatter.alignEntries": false,
  "evenBetterToml.formatter.allowedBlankLines": 1,
  "evenBetterToml.formatter.arrayAutoCollapse": true,
  "evenBetterToml.formatter.arrayAutoExpand": true,
  "evenBetterToml.formatter.arrayTrailingComma": true,
  "evenBetterToml.formatter.columnWidth": 90,
  "evenBetterToml.formatter.trailingNewline": true,
  "explorer.autoReveal": "focusNoScroll",
  "explorer.excludeGitIgnore": true,
  "explorer.fileNesting.enabled": true,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.autoGuessEncoding": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 500,
  "files.eol": "\n",
  "files.exclude": {
    "**/.cache": true,
    "**/.ipynb_checkpoints": true,
    "**/.nuxt/**": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "**/node_modules": true
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.autofetch": "all",
  "git.openRepositoryInParentFolders": "never",
  "jupyter.excludeUserSitePackages": true,
  "jupyter.executionAnalysis.enabled": true,
  "material-icon-theme.activeIconPack": "nest",
  "mypy-type-checker.importStrategy": "fromEnvironment",
  "notebook.formatOnSave.enabled": true,
  "notebook.output.fontSize": 11,
  "notebook.output.scrolling": true,
  "npm-intellisense.showBuildInLibs": true,
  "nuxtr.piniaFiles.defaultTemplate": "setup",
  "nuxtr.vueFiles.firstTag": "script",
  "nuxtr.vueFiles.style.alwaysScoped": true,
  "outline.collapseItems": "alwaysCollapse",
  "prettier.printWidth": 90,
  "prettier.proseWrap": "always",
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.typeCheckingMode": "strict",
  "python.defaultInterpreterPath": "",
  "python.languageServer": "Pylance",
  "python.testing.pytestEnabled": true,
  "ruff.importStrategy": "fromEnvironment",
  "ruff.lineLength": 90,
  "ruff.lint.run": "onSave",
  "scm.alwaysShowRepositories": true,
  "scm.defaultViewMode": "tree",
  "search.smartCase": true,
  "sortJSON.contextMenu": {
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONType": false,
    "sortJSONTypeReverse": false,
    "sortJSONValues": false,
    "sortJSONValuesReverse": false
  },
  "svelte.enable-ts-plugin": true,
  "tailwindCSS.classAttributes": ["class", "className", "ngClass", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui \\=([^;]*);", "'([^']*)'"],
    ["ui \\=([^;]*);", "\"([^\"]*)\""],
    ["ui \\=([^;]*);", "\\`([^\\`]*)\\`"],
    ["ui:\\s*{([^)]*)\\s*}", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["/\\*\\s?ui\\s?\\*/\\s*{([^;]*)}", ":\\s*[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "tailwindCSS.lint.cssConflict": "error",
  "telemetry.telemetryLevel": "off",
  "terminal.explorerKind": "external",
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 12,
  "update.mode": "manual",
  "vue.autoInsert.dotValue": true,
  "vue.splitEditors.icon": true,
  "window.autoDetectHighContrast": false,
  "window.customMenuBarAltFocus": false,
  "window.dialogStyle": "custom",
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Ros√© Pine",
  "workbench.commandPalette.history": 5,
  "workbench.editor.focusRecentEditorAfterClose": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.layoutControl.enabled": false,
  "workbench.panel.opensMaximized": "never",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.reduceMotion": "on",
  "workbench.settings.useSplitJSON": true,
  "workbench.sideBar.location": "right",
  "workbench.tree.renderIndentGuides": "always",
  "zig.initialSetupDone": true,
  "zig.path": "zig"
}
